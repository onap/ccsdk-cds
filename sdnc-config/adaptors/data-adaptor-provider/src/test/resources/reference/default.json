		"sdnc-component-netconf-edit": {
			"derived_from": "tosca.nodes.Component",
			"interfaces": {
				"org.onap.ccsdk.config.adoptor.netconf.NetconfService": {
					"operations": {
						"editConfig": {
							"inputs": {
								"prifix": {
									"required": true,
									"type": "string"
