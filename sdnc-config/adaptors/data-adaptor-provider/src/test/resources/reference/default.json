			"derived_from": "tosca.nodes.Component",
			"interfaces": {
				"org.onap.ccsdk.config.resource.ResourceAssignmentService": {
					"operations": {
						"getResourceAssignment": {
							"inputs": {
								"prifix": {
									"required": true,
