						"user-id": {
							"required": true,
							"type": "string"
						},
						"host-ip-address": {
							"required": true,
							"type": "string"
						},
						"port-number": {
							"required": true,
							"type": "number",
							"default": 830
						},
						"message-time-out": {
							"required": false,
							"type": "number",
							"default": 3000
