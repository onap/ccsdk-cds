{
  "tags": "dynamic-vault-secrets",
  "name": "dynamic-vault-secrets",
  "property": {
    "description": "Vault secret resolution with dynamic endpoint-selector",
    "type": "map",
    "entry_schema": {
      "type": "string"
    }
  },
  "updated-by": "Steve Siani <alphonse.steve.siani.djissitchi@ibm.com>",
  "sources": {
    "input": {
      "type": "source-input"
    },
    "vault-data": {
      "type": "source-rest",
      "properties": {
        "endpoint-selector": "dynamic-secret-source",
        "verb": "GET",
        "type": "JSON",
        "url-path": "$secret-path",
        "path": "/data",
        "input-key-mapping": {
          "secret-path": "secret-path"
        },
        "output-key-mapping": {},
        "key-dependencies": [
          "secret-path"
        ]
      }
    }
  }
}